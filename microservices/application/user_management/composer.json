{
    "name": "sinanturgut/usermanagement",
    "description": "User Management Service",
    "homepage": "https://dev.sinanturgut.com.tr",
    "require": {
        "php": ">=7.2",
        "slim/slim": "^3.9",
        "slim/php-view": "^2.0",
        "illuminate/database": "^8.23",
        "robmorgan/phinx": "dev-master#4c26aeb",
        "vlucas/phpdotenv": "^2.4",
        "tuupola/slim-jwt-auth": "^2.3",
        "respect/validation": "^1.1",
        "league/fractal": "^0.17.0",
        "phpmailer/phpmailer": "^6.0",
      "akrabat/ip-address-middleware": "^1.0",
        "sentry/sdk": "2.0.3"
   },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "UserManagement\\": "src/UserManagement"
        }
    },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout" : 0,
    "php": "7.2"
  },
    "scripts": {
        "start": "php -S localhost:18001 -t public public/index.php",
        "test" : "vendor/bin/phpunit"
    },
    "license": "See License LICENSE",
    "authors": [
        {
            "name": "Sinan Turgut",
            "email": "mail@sinanturgut.com.tr"
        }
    ],
    "minimum-stability": "dev"
}
